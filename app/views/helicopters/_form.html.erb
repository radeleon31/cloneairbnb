<%= simple_form_for(helicopter) do |f| %>
  <%= f.input :price_hour %>
  <%= f.input :occupancy %>
  <%= f.input :autonomy %>
  <%= f.input :category %>
  <%= f.input :city %>
  <%= f.input :description %>
  <%= f.input :status %>
  <%= f.input :photo, as: :file %>
  <%= f.submit :class => "btn btn-primary"  %>
<% end %>
